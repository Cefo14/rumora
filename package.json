{
  "name": "rumora",
  "version": "0.0.1",
  "description": "Zero-tracking web performance monitoring - measure Core Web Vitals, errors, and resource timing without compromising user privacy. TypeScript-first with tree-shaking support.",
  "author": "Cefo14 <cefo14@protonmail.com>",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "web-vitals",
    "performance-monitoring",
    "privacy-first",
    "core-web-vitals",
    "lcp",
    "cls",
    "fcp",
    "fid",
    "inp",
    "rum",
    "real-user-monitoring",
    "performance-observer",
    "web-performance",
    "lighthouse",
    "pagespeed",
    "typescript",
    "tree-shaking",
    "zero-tracking",
    "frontend-monitoring",
    "javascript-performance",
    "performance-analytics",
    "web-vitals-library",
    "performance-metrics"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./web-vitals": {
      "types": "./dist/web-vitals.d.ts",
      "import": "./dist/web-vitals.js",
      "require": "./dist/web-vitals.cjs"
    },
    "./performance": {
      "types": "./dist/performance.d.ts",
      "import": "./dist/performance.js",
      "require": "./dist/performance.cjs"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js",
      "require": "./dist/errors.cjs"
    },
    "./reports": {
      "types": "./dist/reports.d.ts",
      "import": "./dist/reports.js",
      "require": "./dist/reports.cjs"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/test",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cefo14/rumora.git"
  },
  "bugs": {
    "url": "https://github.com/Cefo14/rumora/issues"
  },
  "homepage": "https://cefo14.github.io/rumora",
   "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/cefo14"
  },
  "scripts": {
    "dev": "vite",
    "build:lib": "vite build --mode lib",
    "build:demo": "vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:related": "vitest related",
    "prepare": "husky install",
    "prepublishOnly": "npm run build:lib"
  },
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.33.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jsdom": "21.1.7",
    "@types/node": "24.3.0",
    "@vitest/ui": "3.2.4",
    "eslint": "9.33.0",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jiti": "2.5.1",
    "jsdom": "27.0.0",
    "lint-staged": "16.1.6",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vite": "7.1.3",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4"
  }
}
